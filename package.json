{
  "name": "cache-control-guru",
  "description": "A quick and simple website to help generate and explain cache-control headers.",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "fmt": "SORT_IMPORTS=true prettier --cache --write .",
    "fmt:check": "SORT_IMPORTS=true prettier --check .",
    "lint:fix": "tsc --noEmit && eslint --fix --report-unused-disable-directives --max-warnings 0 .",
    "lint:check": "pnpm run fmt:check && tsc --noEmit && eslint . --report-unused-disable-directives --max-warnings 0",
    "lint": "pnpm run fmt && pnpm run lint:fix"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@solid-primitives/scheduled": "^1.5.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.3",
    "@tanstack/solid-form": "^1.0.5",
    "@tanstack/solid-router": "^1.114.1",
    "@tanstack/zod-adapter": "^1.114.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "solid-js": "^1.9.5",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.5.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.12",
    "@tanstack/router-plugin": "^1.114.1",
    "@tanstack/solid-router-devtools": "^1.114.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
